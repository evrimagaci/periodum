{
  "name": "periodum",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development NUXT_HOST=0.0.0.0 nodemon server/index.js --watch server",
    "build": "NODE_ENV=production nuxt build",
    "heroku-postbuild": "yarn build",
    "start_": "nuxt start",
    "start": "cross-env pm2-runtime start ecosystem.config.js --env production",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest --runInBand"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.1",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "connect-timeout": "^1.9.0",
    "consola": "^2.15.3",
    "core-js": "^3.9.1",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "element-ui": "^2.15.1",
    "express": "^4.17.1",
    "fuse.js": "^6.4.6",
    "helmet": "^4.4.1",
    "install": "^0.13.0",
    "lodash": "^4.17.21",
    "memory-cache": "^0.2.0",
    "mobile-detect": "^1.4.5",
    "mysql2": "^2.2.5",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.15.3",
    "pm2": "^4.5.5",
    "sequelize": "^6.6.1",
    "shrink-ray-current": "^4.1.2",
    "smiles-drawer": "^1.2.0",
    "swiper": "^5.4.5",
    "uuid": "^3.4.0",
    "v-tooltip": "^2.1.3",
    "vue": "^2.6.12",
    "vue-awesome-swiper": "^4.1.1",
    "vue-gtag": "^1.16.1",
    "vue-sanitize": "^0.2.1",
    "vue-slider-component": "^3.2.11",
    "xlsx": "^0.16.9"
